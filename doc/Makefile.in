@DOXYGEN_TRUE@   # Use these to select which kinds of Doxygen documentation
@DOXYGEN_TRUE@   # to generate

#####  Actions

# When we "make doc", what happens?
# Well, we make the doc tarball, so it can be installed
doc: $(docarchive) $(manpages)
	echo "Made the documentation"

# How do we install the doc tarball?
# create the installation directory.
# Then un-tar the doc tarball into that.
# Then remove the doc tarball.
install-data-hook:
	if [ ! -d "$(DESTDIR)$(docdir)" ] ; then mkdir -p "$(DESTDIR)$(docdir)" ; fi
	cd $(DESTDIR)$(docdir) && tar xof $(docarchive) && /bin/rm -f $(docarchive)
# To uninstall the docs?
uninstall-hook:
	cd $(DESTDIR)$(docdir) && /bin/rm -rf $(tar_dirs)
# This should be created when a source file is changed and the library is re-made.
# But just in case, we can also make it here
$(TRIGGER_DOCS_REMAKE): 
	touch $@

# is the Doxygen documentation-generation software enabled?
@DOXYGEN_TRUE@   # We do this only if we can generate documentation files with the Doxygen software

@DOXYGEN_TRUE@gigatraj.dconf: $(TRIGGER_DOCS_REMAKE) $(DOXFILES) gigatraj_base.dconf html_yes.dconf latex_no.dconf man_no.dconf rtf_no.dconf xml_no.dconf perlmod_no.dconf autogen_no.dconf
@DOXYGEN_TRUE@	@cat gigatraj_base.dconf > gigatraj.dconf 
@DOXYGEN_TRUE@	@echo " *** Generating HTML? $(DOX_DO_HTML)"
@DOXYGEN_TRUE@	@cat html_$(DOX_DO_HTML).dconf >> gigatraj.dconf 
@DOXYGEN_TRUE@	@echo " *** Generating LaTeX? $(DOX_DO_LATEX)"
@DOXYGEN_TRUE@	@cat latex_$(DOX_DO_LATEX).dconf >> gigatraj.dconf
@DOXYGEN_TRUE@	@echo " *** Generating man? $(DOX_DO_MAN)"
@DOXYGEN_TRUE@	@cat man_$(DOX_DO_MAN).dconf >>  gigatraj.dconf
@DOXYGEN_TRUE@	@echo " *** Generating rtf? $(DOX_DO_RTF)"
@DOXYGEN_TRUE@	@cat rtf_$(DOX_DO_RTF).dconf >> gigatraj.dconf
@DOXYGEN_TRUE@	@echo " *** Generating XML? $(DOX_DO_XML)"
@DOXYGEN_TRUE@	@cat xml_$(DOX_DO_XML).dconf >> gigatraj.dconf  
@DOXYGEN_TRUE@	@echo " *** Generating perlmod? $(DOX_DO_PERLMOD)"
@DOXYGEN_TRUE@	@cat perlmod_$(DOX_DO_PERLMOD).dconf >> gigatraj.dconf  
@DOXYGEN_TRUE@	@echo " *** Generating Autogen? $(DOX_DO_AUTOGEN)"
@DOXYGEN_TRUE@	@cat autogen_$(DOX_DO_AUTOGEN).dconf >> gigatraj.dconf 

@DOXYGEN_TRUE@$(DOX_TRIGGER): gigatraj.dconf  
@DOXYGEN_TRUE@	rm -rf $(dox_dirs)
@DOXYGEN_TRUE@	mkdir -p $(dox_dirs)
@DOXYGEN_TRUE@	$(DOXYGENRUN) gigatraj.dconf >$(dox_info) 2>&1
@DOXYGEN_TRUE@	echo "Generated" > $(DOX_TRIGGER)

# How do we make the doc tarball?
# We tar up the doc files that we want to install
@DOXYGEN_TRUE@$(docarchive): $(DOX_TRIGGER) $(tar_this)
@DOXYGEN_TRUE@	tar czf $@ $(tar_this)

@DOXYGEN_FALSE@   # We are not doing Doxygen.
@DOXYGEN_FALSE@   # So must instead rely on pre-existing files

@DOXYGEN_FALSE@$(DOX_TRIGGER): 
@DOXYGEN_FALSE@	rm -rf $(nodist_dirs)
@DOXYGEN_FALSE@	echo "Not Generated" > $(DOX_TRIGGER)

# The tarball should already exist
@DOXYGEN_FALSE@$(docarchive): $(DOX_TRIGGER)
@DOXYGEN_FALSE@	if [ -f $(docarchive) ] ; then touch $(docarchive) ; else echo "No tar file?!" ; /bin/false ; fi

$(pdf_out):  $(DOX_TRIGGER)
	if test "x$(DOX_RUN_LATEX)" != "x"  ; then cd latex && make >$(latex_log) 2>&1 && dvipdf refman && cp refman.pdf ../$(pdf_out) ; fi

$(rtf_out):  $(DOX_TRIGGER)
	if test "x$(DOX_DO_RTF)" = "xyes"  ; then  cp rtf/refman.rtf $(rtf_out) ; fi

$(pandir)/gtmodel_s01.3gt:  $(DOX_TRIGGER)
	@echo "man page ready"
$(pandir)/gt_fill_met_cache.3gt:  $(DOX_TRIGGER)
	@echo "man page ready"

html-local: $(DOX_TRIGGER)
	@echo "Generated Doxygen HTML (and possibly other) docs"

pdf-local: $(DOX_TRIGGER)
	@echo "Generated PDF doc"

distclean-local:
	rm -rf $(nodist_dirs)

scrub-clean: clean 
	rm -rf $(dox_dirs) $(dox_logs) $(pdf_out) $(rtf_out) $(noinst_DATA)

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
